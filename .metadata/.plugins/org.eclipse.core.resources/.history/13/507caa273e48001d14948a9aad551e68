package colections;

import java.util.HashSet;
import java.util.Set;

public class ConjuntoBagunçado {
	
	public static void main(String[] args) {
	
		HashSet conjunto = new HashSet();
		// adicionando elementos
		conjunto.add(1.2);
		conjunto.add(true);
		conjunto.add("Teste");
		conjunto.add(1);
		conjunto.add('x');

		//quantificando elementos
		System.out.println(conjunto.size());

		
		//removendo elementos
		System.out.println(conjunto.remove(1.2));
		
		//checando se o conjunto contem o elemento 
		System.out.println(conjunto.contains(false));
		
		// criando um novo conjunto numérico 
		
		Set nums = new HashSet();
		
		nums.add(1);
		nums.add(2);
		nums.add(3);
		
		System.out.println(nums);
		System.out.println(conjunto);
		
	}
}
